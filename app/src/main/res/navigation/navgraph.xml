<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.myproject.ui.fragments.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/map_r" >
        <action
            android:id="@+id/action_mapFragment_to_userFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_mapFragment_to_reviewFragment"
            app:destination="@id/reviewFragment" />
        <action
            android:id="@+id/action_mapFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_mapFragment_to_singinFragment"
            app:destination="@id/singinFragment" />
    </fragment>
    <fragment
        android:id="@+id/singinFragment"
        android:name="com.example.myproject.ui.fragments.singinFragment"
        android:label="fragment_singin"
        tools:layout="@layout/singin_r" >
        <action
            android:id="@+id/action_singinFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_singinFragment_to_reviewFragment"
            app:destination="@id/reviewFragment" />
        <action
            android:id="@+id/action_singinFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_singinFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_singinFragment_to_enterFragment"
            app:destination="@id/enterFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.myproject.ui.fragments.userFragment"
        android:label="fragment_user"
        tools:layout="@layout/user_r" >
        <action
            android:id="@+id/action_accountFragment_to_singinFragment2"
            app:destination="@id/singinFragment" />
        <action
            android:id="@+id/action_accountFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_accountFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_accountFragment_to_reviewFragment"
            app:destination="@id/reviewFragment" />
        <action
            android:id="@+id/action_accountFragment_to_singinFragment"
            app:destination="@id/singinFragment" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.myproject.ui.fragments.welcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_mapFragment2"
            app:destination="@id/mapFragment"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </fragment>
    <fragment
        android:id="@+id/reviewFragment"
        android:name="com.example.myproject.ui.fragments.ReviewFragment"
        android:label="fragment_review"
        tools:layout="@layout/review_r" >
        <action
            android:id="@+id/action_reviewFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_reviewFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_reviewFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_reviewFragment_to_singinFragment"
            app:destination="@id/singinFragment" />
        <action
            android:id="@+id/action_reviewFragment_to_singinFragment2"
            app:destination="@id/singinFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.myproject.ui.fragments.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/search_r" >
        <action
            android:id="@+id/action_searchFragment_to_reviewFragment"
            app:destination="@id/reviewFragment" />
        <action
            android:id="@+id/action_searchFragment_to_accountFragment"
            app:destination="@id/accountFragment" />
        <action
            android:id="@+id/action_searchFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_searchFragment_to_singinFragment"
            app:destination="@id/singinFragment" />
    </fragment>
    <fragment
        android:id="@+id/enterFragment"
        android:name="com.example.myproject.ui.fragments.EnterFragment"
        android:label="fragment_enter"
        tools:layout="@layout/enter" >
        <action
            android:id="@+id/action_enterFragment_to_singinFragment"
            app:destination="@id/singinFragment" />
        <action
            android:id="@+id/action_enterFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.myproject.ui.fragments.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/register" >
        <action
            android:id="@+id/action_registerFragment_to_enterFragment"
            app:destination="@id/enterFragment" />
    </fragment>
</navigation>